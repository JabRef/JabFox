{
	"browser_specific_settings": {
		"gecko": {
			"id": "browserextension@jabref.org"
		}
	},

	"author": "JabRef",

	"background": {
		"scripts": [
			"browser-polyfill.min.js",
			"Zotero/init.js",
			"Zotero/zotero_config.js",
			"Zotero/zotero.js",
			"Zotero/promise.js",
			"Zotero/date.js",
			"Zotero/openurl.js",
			"Zotero/prefs.js",
			"Zotero/errors_webkit.js",
			"Zotero/http.js",
			"Zotero/proxy.js",
			"connector.js",
			"Zotero/cachedTypes.js",
			"Zotero/debug.js",
			"Zotero/xregexp-all.js",
			"Zotero/unicode-zotero.js",
			"Zotero/repo.js",
			"Zotero/translate.js",
			"Zotero/translate_inject.js",
			"Zotero/tlds.js",
			"Zotero/translator.js",
			"Zotero/translators.js",
			"Zotero/connectorTypeSchemaData.js",
			"Zotero/utilities.js",
			"Zotero/utilities_translate.js",
			"Zotero/messages.js",
			"Zotero/messaging.js",
			"Zotero/background.js",
			"globalSettings.js",
			"zsc_misc_pre.js",
			"zsc/zsc.js",
			"zsc_misc_post.js",
			"wsClient.js",
			"background.js"
		]
	},

	"page_action": {
		"default_icon": {
			"48": "data/JabRef-icon-48.png"
		},
		"default_title": "Import references into JabRef",
		"default_popup": "data/progressPanel.html"
	},

	"commands": {
		"_execute_page_action": {
			"suggested_key": {
				"default": "Alt+Shift+J"
			}
		}
	},

	"content_security_policy": "script-src 'self'; object-src 'self'",

	"description": "The JabRef browser extension imports new bibliographic information directly from the browser into JabRef.",

	"developer": {
		"name": "JabRef",
		"url": "http://www.jabref.org/"
	},

	"homepage_url": "http://www.jabref.org/",

	"icons": {
		"48": "data/JabRef-icon-48.png",
		"96": "data/JabRef-icon-96.png"
	},

	"manifest_version": 2,

	"name": "JabRef Browser Extension",

	"options_ui": {
		"page": "data/options.html",
		"browser_style": true,
		"chrome_style": true
	},

	"permissions": [
		"<all_urls>",
		"activeTab",
		"tabs",
		"webNavigation",
		"storage",
		"nativeMessaging"
	],

	"content_scripts": [{
		"matches": ["http://*/*", "https://*/*"],
		"run_at": "document_start",
		"js": [
			"browser-polyfill.min.js",
			"Zotero/zotero_config.js",
			"Zotero/zotero.js",
			"Zotero/promise.js",
			"Zotero/http.js",
			"Zotero/proxy.js",
			"Zotero/cachedTypes.js",
			"Zotero/date.js",
			"Zotero/debug.js",
			"Zotero/openurl.js",
			"Zotero/xregexp-all.js",
			"Zotero/unicode-zotero.js",
			"Zotero/rdf/init.js",
			"Zotero/rdf/uri.js",
			"Zotero/rdf/term.js",
			"Zotero/rdf/identity.js",
			"Zotero/rdf/match.js",
			"Zotero/rdf/rdfparser.js",
			"Zotero/translate.js",
			"Zotero/translator.js",
			"Zotero/translate_item.js",
			"Zotero/connectorTypeSchemaData.js",
			"Zotero/utilities.js",
			"Zotero/utilities_translate.js",
			"Zotero/utilities-common.js",
			"Zotero/http_inject.js",
			"Zotero/progressWindow.js",
			"Zotero/translate_inject.js",
			"Zotero/messages.js",
			"Zotero/messaging_inject.js",
			"Zotero/inject.js",
			"bibtexConverter.js"
		]
	}],

	"version": "2.1.2"
}
